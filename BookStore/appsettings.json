{
  "ConnectionStrings": {
    "PostgreSQL": "User ID=postgres;Password=asdqwe1997;Host=localhost;Port=5432;Database=BookStore"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
